{
    "name": "pagekit/pagekit",
    "type": "project",
    "description": "The Pagekit CMS",
    "keywords": ["cms", "pagekit"],
    "homepage": "http://pagekit.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Pagekit Team",
            "email": "info@pagekit.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/composer": "~1.10.6",
        "doctrine/annotations": "~1.10.2",
        "doctrine/dbal": "~2.10.2",
        "paragonie/random-lib": "~2.0.1",
        "maximebf/debugbar": "~1.16.3",
        "monolog/monolog": "~2.0.2",
        "nikic/php-parser": "~4.4.0",
        "psr/log": "~1.1.3",
        "swiftmailer/swiftmailer": "~6.2",
        "symfony/debug": "~4.4.8",
        "symfony/finder": "~5.1.0",
        "symfony/http-foundation": "~5.0.8",
        "symfony/routing": "~5.0.8",
        "symfony/stopwatch": "~5.0.8",
        "symfony/templating": "~5.0.8",
        "symfony/translation": "~5.0.8",
        "twig/twig": "~3.0.3"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "phpunit/phpunit": "~9.1.4"
    },
    "autoload": {
        "psr-4": {
            "Pagekit\\": "app/modules/application/src",
            "Pagekit\\Installer\\": "app/installer/src",
            "Pagekit\\System\\": "app/system/src",
            "Pagekit\\Config\\": "app/modules/config/src",
            "Pagekit\\Console\\": "app/console/src",
            "Pagekit\\Migration\\": "app/modules/migration/src"
        }
    },
    "config": {
        "vendor-dir": "app/vendor"
    }
}